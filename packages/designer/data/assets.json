          "description": "输入框后置",
          "schema": {
            "componentName": "Icon"
          }
        }
      ],
      "events": [
        {
          "name": "input",
          "title": "输入",
          "description": "输入框值变化时触发",
          "arguments": [
            {
              "name": "value",
              "type": "String",
              "description": "输入框值"
            }
          ]
        },
        {
          "name": "change",
          "title": "值变化",
          "description": "仅当 modelValue 改变时，当输入框失去焦点或用户按Enter时触发",
          "arguments": [
            {
              "name": "value",
              "type": "String",
              "description": "输入框值"
            }
          ]
        },
        {
          "name": "blur",
          "title": "失焦",
          "description": "输入框失焦时触发",
          "arguments": [
            {
              "name": "event",
              "type": "Event",
              "description": "原生事件对象"
            }
          ]
        },
        {
          "name": "focus",
          "title": "聚焦",
          "description": "输入框聚焦时触发",
          "arguments": [
            {
              "name": "event",
              "type": "Event",
              "description": "原生事件对象"
            }
          ]
        },
        {
          "name": "clear",
          "title": "清空",
          "description": "点击清空按钮时触发",
          "arguments": [
            {
              "name": "event",
              "type": "Event",
              "description": "原生事件对象"
            }
          ]
        }
      ]
    }
  },
