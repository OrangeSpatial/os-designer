      ],
      "slots": [
        {
          "name": "prefix",
          "title": "前缀",
          "description": "输入框前缀",
          "schema": {
            "componentName": "Icon"
          }
        },
        {
          "name": "suffix",
          "title": "后缀",
          "description": "输入框后缀",
          "schema": {
            "componentName": "Icon"
          }
        },
        {
          "name": "prepend",
          "title": "前置",
          "description": "输入框前置",
          "schema": {
            "componentName": "Icon"
          }
        },
        {
          "name": "append",
